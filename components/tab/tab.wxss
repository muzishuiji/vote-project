.foot-bar {
  box-sizing: border-box;
  position: absolute;
  left: 0;
  bottom: 0;
  height: 45px;
  background: #fff;
  border-top: 1px solid #ddd;
 
  
}
.tab-container {
  width: 100vw;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
}
.navigator-normal {
  flex: 1;
  text-align: center;
  padding-top: 5px;
}

.nav-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  line-height: 22px;
  font-size:14px;
  color: #aaaaaa;
}
.tab-icon{
  width: 20px;height:20px;
}
.navigator-active {
  color:#0186fa;
  
}