/*global style*/

.fr {
  float: right;
}
.gray {
  font-size: 12px;
  color: #aaaaaa;
}
.title {
  font-size: 16px;
  font-weight: bold;
  color: #333;
  line-height: 30px;
}
.blue {
  color: #0186fa;
  
}
.yellow {
  color: #FF9800;
}
.yellow-bg {
  background: #FF9800;
}
.blue2 {
  font-size: 14px;
}
.blue1 {
  font-size: 14px;
}
.big-size {
  font-size: 18px;
}

.fl {
  float: left;
}
.text-right {
  text-align: right;
}
.text-center {
  text-align: center;
}
.container {
  position: relative;
  width: 100vw;
  height: 100vh;
}


.foot-bar {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100vw;
  height: 50px;
  display: flex;
  display: -webkit-flex;
  background: #fff;
  border-top: 1px solid #ddd;
}
.navigator-normal {
  flex: 1;
  text-align: center;
  line-height: 48px;
}